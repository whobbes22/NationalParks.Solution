@{
  ViewData["Title"] = $"Delete {Model.ParkName}";
}

@model ParkClient.Models.Park

<h4>Are you sure you want to delete @Model.ParkName?</h4>

@using (Html.BeginForm())
{
  <input type="submit" value="Delete" class="btn btn-primary"/>
}

@Html.ActionLink("Back to details", "Details", new { id = @Model.ParkId})